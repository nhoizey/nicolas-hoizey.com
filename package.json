    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "fast-glob": "^3.2.2",
    "html-entities": "^1.3.1",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
