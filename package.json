{
  "name": "nicolas-hoizey.com",
  "version": "2.3.0",
  "title": "Nicolas Hoizey, passionate about the Web and photography",
  "description": "This is the personnal website of Nicolas Hoizey, passionate about the Web and photography, among many other interests.",
  "author": {
    "name": "Nicolas Hoizey",
    "email": "nicolas@hoizey.com",
    "url": "https://nicolas-hoizey.com/"
  },
  "license": "MIT",
  "homepage": "https://nicolas-hoizey.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhoizey/nicolas-hoizey.com"
  },
  "scripts": {
    "clean": "rimraf _site",
    "assets:build": "rollup -c",
    "assets:watch": "rollup -c --watch",
    "eleventy:build": "cross-env NODE_ENV=production eleventy",
    "eleventy:watch": "cross-env NODE_ENV=development eleventy --serve",
    "serviceworker": "workbox injectManifest workbox.config.js",
    "build": "npm-run-all --sequential clean assets:build eleventy:build serviceworker",
    "debug:build": "DEBUG=Eleventy* npm run build",
    "start": "npm-run-all --sequential clean assets:build --parallel *:watch",
    "debug:start": "DEBUG=Eleventy* npm start",
    "notist": "node ./src/_utils/update-notist.js"
  },
