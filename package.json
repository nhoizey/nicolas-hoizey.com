{
  "name": "nicolas-hoizey.com",
  "version": "2.3.0",
  "title": "Nicolas Hoizey, passionate about the Web and photography",
  "description": "This is the personnal website of Nicolas Hoizey, passionate about the Web and photography, among many other interests.",
  "author": {
    "name": "Nicolas Hoizey",
    "email": "nicolas@hoizey.com",
    "url": "https://nicolas-hoizey.com/"
  },
  "license": "MIT",
  "homepage": "https://nicolas-hoizey.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhoizey/nicolas-hoizey.com"
  },
  "scripts": {
    "clean": "rimraf _site",
    "svg": "node src/_utils/generate-svg-sprite.js",
    "assets:build": "rollup -c",
    "assets:watch": "rollup -c --watch",
    "eleventy:build": "eleventy",
    "eleventy:watch": "eleventy --serve",
    "serviceworker": "workbox injectManifest workbox.config.js",
    "lambdas:build": "netlify-lambda build _lambdas",
    "lambdas:call": "netlify functions:invoke --name deploy-succeeded",
    "build": "cross-env NODE_ENV=production npm-run-all --sequential clean assets:build eleventy:build serviceworker",
    "build:netlify": "cross-env NODE_ENV=production npm-run-all --sequential build lambdas:build",
    "benchmark:build": "DEBUG=Eleventy:Benchmark* cross-env NODE_ENV=production npm-run-all --sequential clean assets:build eleventy:build serviceworker",
    "debug:build": "DEBUG=Eleventy* npm run build",
    "start": "cross-env NODE_ENV=development npm-run-all --sequential clean --parallel *:watch",
    "debug:start": "DEBUG=Eleventy* npm start",
    "notist": "node ./src/_utils/update-notist.js",
    "webmention": "node ./src/_utils/update-webmention.js",
    "algolia": "atomic-algolia"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@jlengstorf/get-share-image": "^0.7.1",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@sindresorhus/slugify": "~1.1",
    "babel-loader": "^8.1.0",
    "bent": "^7.3.9",
    "cheerio": "~1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eleventy-plugin-embed-tweet": "^0.3.0",
    "eleventy-plugin-images-responsiver": "^1.5.3",
    "encoding": "^0.1.13",
    "entities": "^2.0.3",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "fast-glob": "^3.2.4",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lodash": "^4.17.19",
    "markdown-it": "~11.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "~5.3",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "~3.0",
    "markdown-it-footnote": "~3.0.2",
    "moment": "~2.27.0",
    "netlify-build-plugin-dareboost": "^1.2.1",
    "netlify-lambda": "^2.0.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "2.0.5",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-entrypoint-hashmanifest": "^0.1.2",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-terser": "^6.1.0",
    "sanitize-html": "^1.27.2",
    "svgstore": "^3.0.0-2",
    "truncate-html": "^1.0.3",
    "twitter": "^1.7.1",
    "twitter-text": "^3.1.0",
    "workbox-broadcast-update": "5.1.3",
    "workbox-cacheable-response": "5.1.3",
    "workbox-cli": "^5.1.3",
    "workbox-core": "5.1.3",
    "workbox-expiration": "5.1.3",
    "workbox-google-analytics": "5.1.3",
    "workbox-precaching": "5.1.3",
    "workbox-routing": "5.1.3",
    "workbox-strategies": "5.1.3"
  },
  "engines": {
    "node": ">=13"
  },
  "private": true,
  "dependencies": {}
}
