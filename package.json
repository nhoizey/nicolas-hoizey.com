{
  "name": "nicolas-hoizey.com",
  "version": "2.1.0",
  "title": "Nicolas Hoizey, passionate about the Web and photography",
  "author": {
    "name": "Nicolas Hoizey",
    "email": "nicolas@hoizey.com",
    "url": "https://nicolas-hoizey.com/",
    "twitter": "nhoizey",
    "github": "nhoizey"
  },
  "license": "MIT",
  "homepage": "https://nicolas-hoizey.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhoizey/nicolas-hoizey.com"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "js:build": "rollup --config rollup-js.config.js",
    "js:watch": "rollup --config rollup-js.config.js --watch",
    "css-task:sass-critical": "node-sass src/_assets/sass/critical.scss src/_includes/generated/critical.css",
    "css-task:sass-additional": "node-sass src/_assets/sass/additional.scss dist/css/additional.css --output-style compressed",
    "css:build": "npm-run-all -p css-task:*",
    "css:watch": "chokidar 'src/_assets/sass/**/*.scss' -c 'npm run css:build'",
    "assets:build": "npm-run-all -p css:build js:build",
    "sw:build": "rollup --config rollup-serviceworker.config.js && workbox injectManifest ./workbox.config.js",
    "eleventy:build": "cross-env NODE_ENV=production eleventy",
    "eleventy:serve": "cross-env NODE_ENV=development eleventy --serve",
    "start": "npm-run-all -p css:* js:watch eleventy:serve",
    "startdebug": "DEBUG=Eleventy* npm start",
    "build": "npm-run-all --sequential assets:build eleventy:build sw:build",
    "builddebug": "DEBUG=Eleventy* npm run build"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0-beta.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@jlengstorf/get-share-image": "^0.7.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@sindresorhus/slugify": "~0.11",
    "babel-loader": "^8.1.0",
    "cheerio": "~1.0.0-rc.3",
    "chokidar-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eleventy-plugin-images-responsiver": "^1.5.1",
    "eleventy-plugin-svg-contents": "~0.5.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fast-glob": "^3.2.2",
    "html-entities": "^1.2.1",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lodash": "^4.17.15",
    "markdown-it": "~10.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "~5.2",
    "markdown-it-attrs": "^3.0.2",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "~2.0",
    "markdown-it-footnote": "~3.0.2",
    "moment": "~2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "2.0.2",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "sanitize-html": "^1.22.1",
    "truncate-html": "^1.0.3",
    "twitter-text": "^3.1.0",
    "workbox-broadcast-update": "5.1.2",
    "workbox-cacheable-response": "5.1.2",
    "workbox-cli": "^5.1.2",
    "workbox-core": "5.1.2",
    "workbox-expiration": "5.1.2",
    "workbox-google-analytics": "5.1.2",
    "workbox-precaching": "5.1.2",
    "workbox-routing": "5.1.2",
    "workbox-strategies": "5.1.2"
  },
  "engines": {
    "node": ">=8.16"
  },
  "private": true
}
